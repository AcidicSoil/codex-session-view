{
  "name": "codex-session-view",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:vercel": "node tools/vercel-dev.mjs",
    "build": "vite build",
    "preview": "vite preview --host 127.0.0.1 --port 4173",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:prod": "pnpm build && PLAYWRIGHT_USE_PROD=1 pnpm playwright test --project=chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "pnpm build && pnpm test:e2e",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "rules:apply": "ruler apply --nested",
    "prepare": "ruler apply --nested",
    "code": "codefetch --include-dir src --exclude-dir .ruler,.cursor --exclude-files \"*.test.ts\",*.md, -o src.md",
    "ui": "codefetch -t 5 --include-dir src/components/ --exclude-dir .ruler,.cursor --exclude-files \"*.test.ts\",*.md, -o ui.md",
    "code:agents": "codefetch --include-dir codefetch/agents/ --include-files '*.md' -o src-code_agents.md",
    "code:srca": "pnpm code && pnpm code:agents",
    "agents": "bash scripts/agents-v2.sh && pnpm code:srca",
    "thres": "git ls-files --cached --others --exclude-standard -z | xargs -0 -n 1 wc -l | awk '$1 >= 400 { printf \"%s\\t%s\\n\", $2, $1 }'",
    "tasks": "codefetch --include-dir docs/tasks/ -o tasks.md"
  },
