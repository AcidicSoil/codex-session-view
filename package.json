{
  "name": "codex-session-view",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --host 127.0.0.1 --port 4173",
    "start": "node .output/server/index.mjs",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:prod": "pnpm build && PLAYWRIGHT_USE_PROD=1 pnpm playwright test --project=chromium",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "pnpm build && pnpm test:e2e",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "rules:apply": "ruler apply --nested",
    "prepare": "ruler apply --nested",
    "code": "codefetch --include-dir src --exclude-dir .ruler,.cursor -o src.md",
    "code:task": "codefetch --include-dir .taskmaster --exclude-dir .taskmaster/docs -o tm.md",
    "code:routes": "codefetch --include-dir src/routes --exclude-dir .ruler,.cursor -o routes.md",
    "code:hooks": "codefetch --include-dir src/hooks --exclude-dir .ruler,.cursor -o hooks.md",
    "code:serve": "codefetch --include-dir src/server --exclude-dir .ruler,.cursor -o server.md",
    "code:all": "pnpm code:src && pnpm code:comp && pnpm code:routes && pnpm code:hooks && pnpm code:serve"
  },
